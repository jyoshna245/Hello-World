    <validationRules>
        <fullName>RFA_DOA_Amount_UpperLimit_Exceeded</fullName>
        <active>true</active>
        <description>DOA Amount should not exceed 25</description>
        <errorConditionFormula>Amount_Lower_limit__c  &gt; Amount_Upper_limit__c || Amount_Lower_limit__c &lt; 0 || Amount_Lower_limit__c &lt; 0</errorConditionFormula>
        <errorMessage>Amount (Upper limit) and Amount (Lower limit) should be non negative values.          
Amount (Lower limit) should not be greater than Amount (Upper limit)</errorMessage>
    </validationRules>
    <validationRules>
