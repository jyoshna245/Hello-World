        <externalId>false</externalId>
        <label>Fixed Filter Expression</label>
        <required>false</required>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Variable_Filter_Expression__c</fullName>
        <description>Value is dependent on filters setup in the lookup fields

ex: User = &apos;{!param1}&apos;</description>
        <externalId>false</externalId>
        <label>Variable Filter Expression</label>
        <required>false</required>
        <type>TextArea</type>
    </fields>
    <label>GS1 Lookup Definitions</label>
</CustomObject>
