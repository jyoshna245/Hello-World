	if(records.length&lt;1) {
		 Utils.Dialog.alert(&quot;Please choose at least one GS1 Request to send back.&quot;,&apos;Error&apos;);
        } else if (isIE &amp;&amp; (records.length &gt; 100)) {
                 Utils.Dialog.alert(&quot;You can send back only up 100 records.&quot;,&apos;Error&apos;);         
	} else {
		 Utils.Dialog.confirm(&quot;The selected requests will be sent back\n\nDo you wish to continue?&quot;,&quot;Warning&quot;, function() {
			   window.location.href=&quot;/apex/GS1_SendBack?ids=&quot;+records+&apos;&amp;srcview=&apos;+getViewId();
			}, true);
	}
}

function getViewId() {
    var cbo = GS1.getViewComboList();
    if (cbo) {
       if (cbo.id != &apos;&apos;) {
          return cbo.id.split(&apos;_&apos;)[0];
       }
    }
